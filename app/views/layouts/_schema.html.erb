<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "Organization",
    "name": "East Bay DSA",
    "alternateName": [
      "East Bay Democratic Socialists of America",
      "DSA East Bay",
      "EBDSA"
    ],
    "url": "http://www.eastbaydsa.org",
    "email": "info@eastbaydsa.org",
    "description": "{{ page.excerpt }}",
    "logo": "https://d3n8a8pro7vhmx.cloudfront.net/eastbaydsa/pages/1/attachments/original/1498456579/east-bay-dsa-logo.png",
    "sameAs": [
      "https://www.facebook.com/eastbaydsa/",
      "https://www.twitter.com/dsaeastbay/",
      "https://vimeo.com/eastbaydsa"
    ]
  }
</script>

<!-- {% if page.level > 0 %}
