{% assign position = 1 %} -->

<script type="application/ld+json">
{
"@context": "http://schema.org",
"@type": "BreadcrumbList",
"itemListElement": [{
  "@type": "ListItem",
  "position": {{ position }},
  "item": {
    "@id": "http://www.eastbaydsa.org",
    "name": "East Bay DSA"
  }
}{% for ancestor in page.ancestors %}{% if ancestor.is_homepage? == false %},{
    "@type": "ListItem",
    "position": {{ forloop.index0 | plus: 2 }},
    "item": {
      "@id": "http://www.eastbaydsa.org{{ ancestor.url }}",
      "name": "{{ ancestor.name }}"
    }
  }{% endif %}{% endfor %}{% if page.is_homepage? == false %},{
    "@type": "ListItem",
    "position": {{ page.level | plus: 2 }},
    "item": {
      "@id": "http://www.eastbaydsa.org{{ page.url }}",
      "name": "{{ page.name }}"
    }
  }{% endif %}
]
}
</script>

<%= yield :schema %>
